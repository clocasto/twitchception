{
  "name": "twitchception",
  "version": "0.0.1",
  "description": "A multi-stream (picture-in-picture) viewing client for popular social video streaming platform, Twitch.tv.",
  "main": "app.js",
  "scripts": {
    "redis:dev": "redis-server server/env/modes/development/redis.conf",
    "redis:prod": "redis-server server/env/modes/production/redis.conf",
    "clean-dist": "rm -rf public/dist/ && mkdir public/dist",
    "build:dev": "npm run clean-dist && webpack -d --watch --display-error-details",
    "build:prod": "npm run clean-dist && webpack -p",
    "lint": "node_modules/eslint/bin/eslint.js \"server/**\"",
    "test": "node_modules/.bin/nyc --require babel-core/register ./node_modules/mocha/bin/_mocha tests --recursive",
    "run-coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/coveralls/bin/coveralls.js",
    "clean-coverage": "rm -rf .nyc_output && rm -rf coverage",
    "coverage": "npm run run-coverage && npm run clean-coverage",
    "pack": "npm run lint && npm run build:prod && npm test",
    "deploy": "npm run redis:prod & export NODE_ENV=production && node app.js",
    "start": "npm run build:dev && npm run redis:dev & export NODE_ENV=development && nodemon app.js"
  },
  "keywords": [],
  "contributors": [
    "Alex Bazhenov (https://github.com/Abazhenov)",
    "Chris LoCasto (https://github.com/clocasto)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/clocasto/twitchception"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "helmet": "^3.0.0",
    "morgan": "^1.7.0",
    "muicss": "^0.9.9-rc2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pg-native": "^1.10.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-draggable": "^2.2.2",
    "react-fa": "^4.1.2",
    "react-fontawesome": "^1.2.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redis": "^2.6.2",
    "redis-server": "0.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.25.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  }
}
